Tabs.FE4:AddToggle("AutoCommand4", {
    Title = "ğŸ”„ è‡ªå‹•ã‚³ãƒãƒ³ãƒ‰é€ä¿¡ï¼ˆé€£ç¶šï¼‰",
    Description = "å®šæœŸçš„ã«7ã¤ã®ã‚³ãƒãƒ³ãƒ‰ã‚’é€ä¿¡",
    Default = false,
    Callback = function(Value)
        _G.AutoCommand4 = Value
        spawn(function()
            local commands = {
                "-gh 139904067056008",
                "-gh 108186273151388",
                "-gh 72292903231768",
                "-gh 84451219120140",
                "-gh 95290698984301",
                "-gh 14254923976",
                "-gh 17163596085"
            }
            
            while _G.AutoCommand4 do
                for i, cmd in ipairs(commands) do
                    if not _G.AutoCommand4 then break end
                    sendChat(cmd)
                    wait(0.5)
                end
                wait(15)
            end
        end)
        
        if Value then
            Fluent:Notify({
                Title = "ğŸ”„ è‡ªå‹•é€ä¿¡ON",
                Content = "15ç§’ã”ã¨ã«å…¨ã‚³ãƒãƒ³ãƒ‰ã‚’é€ä¿¡ã—ã¾ã™",
