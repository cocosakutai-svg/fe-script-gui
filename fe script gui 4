local commands = {
    {name = "ã‚³ãƒãƒ³ãƒ‰1", cmd = "-gh 139904067056008"},
    {name = "ã‚³ãƒãƒ³ãƒ‰2", cmd = "-gh 108186273151388"},
    {name = "ã‚³ãƒãƒ³ãƒ‰3", cmd = "-gh 72292903231768"},
    {name = "ã‚³ãƒãƒ³ãƒ‰4", cmd = "-gh 84451219120140"},
    {name = "ã‚³ãƒãƒ³ãƒ‰5", cmd = "-gh 95290698984301"},
    {name = "ã‚³ãƒãƒ³ãƒ‰6", cmd = "-gh 14254923976"},
    {name = "ã‚³ãƒãƒ³ãƒ‰7", cmd = "-gh 17163596085"}
}

for i, data in ipairs(commands) do
    Tabs.FE4:AddButton({
        Title = "ğŸ“¤ " .. data.name,
        Description = data.cmd,
        Callback = function()
            sendChat(data.cmd)
            Fluent:Notify({
                Title = "âœ… é€ä¿¡",
                Content = data.cmd,
                Duration = 3
            })
        end
    })
end

